{
  "name": "lflt-core",
  "version": "0.0.4",
  "description": "Angular components and directives for leaflet",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "npm run build --prod",
    "postversion": "cd dist/lflt-core && npm version $(node -p -e \"require('../../package.json').version\")",
    "publish_dist": "cd dist/lflt-core && npm publish",
    "deploy": "ng build --prod && npm run publish_dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kygoh/lflt-core.git"
  },
  "bugs": {
    "url": "https://github.com/kygoh/lflt-core/issues"
  },
  "homepage": "https://github.com/kygoh/lflt-core",
  "keywords": [
    "angular",
    "leaflet"
  ],
  "author": "Goh Keng Yew",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@kygoh"
  },
  "dependencies": {
    "@angular/animations": "~9.0.5",
    "@angular/common": "~9.0.5",
    "@angular/compiler": "~9.0.5",
    "@angular/core": "~9.0.5",
    "@angular/forms": "~9.0.5",
    "@angular/platform-browser": "~9.0.5",
    "@angular/platform-browser-dynamic": "~9.0.5",
    "@angular/router": "~9.0.5",
    "@types/leaflet": "^1.5.17",
    "leaflet": "^1.6.0",
